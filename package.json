  "private": false,
  "scripts": {
    "start": "grunt server",
    "test": "grunt test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nlp-compromise/react-nlp.git"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.0.20",
    "babel-loader": "^6.0.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-polyfill": "^6.1.19",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babelify": "7.2.*",
    "chai": "^3.3.0",
    "esformatter-asi": "^0.5.2",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^4.2.3",
    "grunt": "^0.4.0",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^4.0.0",
    "grunt-contrib-uglify": "^0.11.1",
    "grunt-contrib-watch": "^0.6.0",
    "grunt-filesize": "^0.0.7",
    "grunt-react": "^0.12.0",
    "grunt-run": "^0.5.0",
    "mocha": "^2.3.3",
    "react": "^0.14.3",
    "react-datepicker": "^0.18.0",
    "react-dom": "^0.14.3",
    "react-select": "^0.9.1"
  },
  "dependencies": {
    "nlp_compromise": "latest"
  },
  "esformatter": {
    "plugins": [
      "esformatter-quotes",
      "esformatter-jsx",
      "esformatter-parseint",
      "esformatter-braces",
      "esformatter-remove-trailing-commas"
    ],
    "quotes": {
      "type": "single",
      "avoidEscape": false
    },
    "whiteSpace": {
      "before": {
        "ParameterList": -1,
        "ParameterComma": -1,
        "FunctionDeclarationOpeningBrace": 1,
        "FunctionDeclarationClosingBrace": 1,
        "ForStatementExpressionOpening": 1
      },
      "after": {
        "FunctionName": 1,
        "ParameterComma": 1,
        "FunctionReservedWord": 1,
        "ParameterList": -1,
        "FunctionDeclarationOpeningBrace": 1,
        "PropertyName": -1
      }
    },
    "lineBreak": {
      "before": {
        "EndOfFile": 1
      }
    },
    "jsx": {
      "formatJSX": false
    }
  }
}
